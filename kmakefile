.PHONY: all
all: _all

.NOTPARALLEL:

-include Kmakefile
.SILENT: Kmakefile
Kmakefile:
	echo "kmake: Use $@ to customize build." >&2

objtree	:= .
obj	:= $(objtree)

ifeq ("$(origin O)", "command line")
obj := $(O)
endif

include runqemu.mk

%: MAKEFLAGS += --no-print-directory
%:
	@$(MAKE) $@

